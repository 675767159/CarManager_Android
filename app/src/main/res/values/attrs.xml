<?xml version="1.0" encoding="utf-8"?>
<resources>


    <declare-styleable name="NavBarView">
        <attr name="title" format="string" />
        <attr name="background_self_define" format="reference" />
        <attr name="hiddenBackButton" format="boolean"/>
        <attr name="hiddenConnectButton" format="boolean"/>

    </declare-styleable>

    <declare-styleable name="RegisterInputView">
        <attr name="name" format="string" />
        <attr name="description" format="string" />
        <attr name="hiddenLine" format="boolean"/>
        <attr name="isPassword" format="boolean"/>

    </declare-styleable>


    <declare-styleable name="HomeItemView">
        <attr name="title1" format="string"/>
        <attr name="title2" format="string"/>
        <attr name="title3" format="string"/>
        <attr name="hiddenTitle2" format="boolean"/>
        <attr name="image" format="reference" />
    </declare-styleable>

    <declare-styleable name="MenuItemView">
        <attr name="text" format="string"/>
        <attr name="HomeItemView_image" format="reference" />
    </declare-styleable>


    <declare-styleable name="EditCarInputView">
        <attr name="EditCar_title" format="string" />
        <attr name="EditCar_title_description" format="string" />
        <attr name="EditCar_hint" format="string"/>
        <attr name="EditCar_canEdit" format="boolean"/>
        <attr name="EditCar_isButton" format="boolean"/>

    </declare-styleable>

</resources>